---
layout: null
---
[
  {% for doc in site.docs %}
    {
      "title"    : "{{ doc.title | escape }}",
      "category" : "{{ doc.category }}",
      "tags"     : "{{ doc.tags | join: ', ' }}",
      "url"      : "{{ site.baseurl }}{{ doc.url }}",
      "date"     : "{{ doc.date }}",
      "body"     : "{{ page.content | strip_html | strip_newlines | replace: '\', '\\\\' | replace: '"', '\\"' | replace: '	', '    ' }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
